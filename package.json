{
  "name": "em-nome-da-lai",
  "version": "0.0.1",
  "description": "Em nome da LAI! Gerador de petições com base na LAI.",
  "scripts": {
    "start": "elm-live src/Main.elm --start-page=index.html --dir=dist/ --path-to-elm=node_modules/.bin/elm --open --hot -- --output=dist/app.js --debug",
    "build": "elm make src/Main.elm --optimize --output=dist/app.js",
    "test": "elm-format src/ --validate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cuducos/em-nome-da-lai.git"
  },
  "keywords": [
    "lai",
    "dados abertos",
    "brasil",
    "lei de acesso à informação"
  ],
  "author": "Eduardo Cuducos",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/cuducos/em-nome-da-lai/issues"
  },
  "homepage": "https://github.com/cuducos/em-nome-da-lai#readme",
  "devDependencies": {
    "elm": "^0.19.1-3",
    "elm-format": "^0.8.5",
    "elm-live": "^4.0.2"
  }
}
